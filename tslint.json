{
  "extends": [],
  "defaultSeverity": "warning",
  "rules": {
    "max-line-length": {
      "options": [
        120
      ]
    },
    "trailing-comma": [true, "ignore"],
    "member-access": [ true, "no-public" ],
    "semicolon": [
      true,
      "never"
    ],
    "no-unused-variable":false,
    "no-unused-expression":false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
